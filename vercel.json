{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    { "src": "api/css/(.*)", "dest": "api/css/$1" },
    { "src": "api/img/(.*)", "dest": "api/img/$1" },
    { "src": "api/js/(.*)", "dest": "api/js/$1" },
    { "src": "api/lib/(.*)", "dest": "api/lib/$1" },
    { "src": "api/scss/(.*)", "dest": "api/scss/$1" },
    { "src": "/(.*)", "dest": "/api/$1" }
  ]
}
