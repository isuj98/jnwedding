{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    { "src": "/css/(.*)", "dest": "api/css/$1" },
    { "src": "/img/(.*)", "dest": "api/img/$1" },
    { "src": "/js/(.*)", "dest": "api/js/$1" },
    { "src": "/lib/(.*)", "dest": "api/lib/$1" },
    { "src": "/scss/(.*)", "dest": "/scss/$1" },
    { "src": "/(.*)", "dest": "/api/$1" }
  ]
}
